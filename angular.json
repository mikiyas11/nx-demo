{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "analytics": "8dea41d9-1324-4a92-8697-358269caba5e"
  },
  "defaultProject": "nx-demo",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "nx-demo": {
      "projectType": "application",
      "root": "apps/nx-demo",
      "sourceRoot": "apps/nx-demo/src",
      "prefix": "nx-demo",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/nx-demo",
            "index": "apps/nx-demo/src/index.html",
            "main": "apps/nx-demo/src/main.ts",
            "polyfills": "apps/nx-demo/src/polyfills.ts",
            "tsConfig": "apps/nx-demo/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/nx-demo/src/favicon.ico",
              "apps/nx-demo/src/assets"
            ],
            "styles": [
              "apps/nx-demo/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/nx-demo/src/environments/environment.ts",
                  "with": "apps/nx-demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "nx-demo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "nx-demo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "nx-demo:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/nx-demo/src/**/*.ts",
              "apps/nx-demo/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/nx-demo"
          ],
          "options": {
            "jestConfig": "apps/nx-demo/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "nx-demo-e2e": {
      "root": "apps/nx-demo-e2e",
      "sourceRoot": "apps/nx-demo-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/nx-demo-e2e/cypress.json",
            "tsConfig": "apps/nx-demo-e2e/tsconfig.e2e.json",
            "devServerTarget": "nx-demo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "nx-demo:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/nx-demo-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "ui-calendar": {
      "projectType": "library",
      "root": "libs/ui-calendar",
      "sourceRoot": "libs/ui-calendar/src",
      "prefix": "nx-demo",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ui-calendar/src/**/*.ts",
              "libs/ui-calendar/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/ui-calendar"
          ],
          "options": {
            "jestConfig": "libs/ui-calendar/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/angular",
            "port": 4400,
            "config": {
              "configFolder": "libs/ui-calendar/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "uiFramework": "@storybook/angular",
            "outputPath": "dist/storybook/ui-calendar",
            "config": {
              "configFolder": "libs/ui-calendar/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "ui-calendar-e2e": {
      "root": "apps/ui-calendar-e2e",
      "sourceRoot": "apps/ui-calendar-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ui-calendar-e2e/cypress.json",
            "tsConfig": "apps/ui-calendar-e2e/tsconfig.e2e.json",
            "devServerTarget": "ui-calendar:storybook"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "ui-calendar:storybook:ci"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/ui-calendar-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    }
  }
}